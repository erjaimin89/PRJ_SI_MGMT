	<queryString>
		<![CDATA[select sims.PURCHASE_ORDERS_DETAILS.`ITEM_DETAIL_NAME`,
		sims.PURCHASE_ORDERS_DETAILS.`ITEM_NAME`,
		sims.PURCHASE_ORDERS_DETAILS.`PARTY_NAME`,
		sum(sims.PURCHASE_ORDERS_DETAILS.`SIZE_1`),
        sum(sims.PURCHASE_ORDERS_DETAILS.`SIZE_2`),
        sum(sims.PURCHASE_ORDERS_DETAILS.`SIZE_3`),
        sum(sims.PURCHASE_ORDERS_DETAILS.`SIZE_4`),
        sum(sims.PURCHASE_ORDERS_DETAILS.`SIZE_5`),
        sum(sims.PURCHASE_ORDERS_DETAILS.`SIZE_6`),
        sum(sims.PURCHASE_ORDERS_DETAILS.`SIZE_7`),
        sum(sims.PURCHASE_ORDERS_DETAILS.`SIZE_8`),
       sum(sims.PURCHASE_ORDERS_DETAILS.`TOTAL_QUANTITY`) 
from sims.PURCHASE_ORDERS 
     INNER JOIN sims.PURCHASE_ORDERS_DETAILS ON
     sims.PURCHASE_ORDERS.`ID` =  sims.PURCHASE_ORDERS_DETAILS.`PURCHASE_ORDER_ID`
where sims.PURCHASE_ORDERS.`SHIPPING_STATUS`=1  and $P!{whereClause}
group by sims.PURCHASE_ORDERS_DETAILS.`ITEM_DETAIL_ID`]]>
	</queryString>
